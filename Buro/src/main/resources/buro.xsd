<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
elementFormDefault="qualified" 
attributeFormDefault="unqualified"
targetNamespace="http://soa.com">

	<xs:element name="consultarBuroRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id_folio" type="xs:int"></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="consultarBuroResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="aprobado" type="xs:boolean"></xs:element>
				<xs:element name="tasa" type="xs:double"></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>
